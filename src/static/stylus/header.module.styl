.headerDiv
  display: flex
  align-items: center
  background-color: rgb(193, 161, 106)
  color: black
  width: 100%
  padding: 0 1% 5px 1%
  min-height: 50px
  height: 5%

.logo
  display: inline-block
  width: 5%
  max-height: 50px
  margin-top: 5px

.company
  font-size: 2em
  vertical-align: text-bottom
  display: inline-block
  width: 24%
  position: relative

.linkText
  text-decoration: none
  color: black

.search
  display: flex
  margin: auto
  width: 40%

.search > input
  color: grey
  opacity: 0.25
  font-size: 1.5em
  padding: 5px
  border-radius: 7px
  border: none
  width: 100%

.nav
  display: flex
  display: inline
  width: 30%

.linkImg
  padding: 0 10px 0 10px




















